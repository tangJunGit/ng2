{
    "name": "ng2",
    "version": "0.0.1",
    "description": "Angular 2 Webpack2",
    "keywords": [
        "angular2",
        "webpack",
        "typescript"
    ],
    "author": "tangjun <tj_910601@163.com>",
    "scripts": {
        "build:prod": "webpack --config config/webpack.prod.js  --progress --profile --bail",
        "build:aot": "webpack --config config/webpack.aot.js  --progress --profile --bail",
        "build": "npm run build:prod",
        "clean:dist": "npm run rimraf dist",
        "clean:ngc": "npm run rimraf ngfactory aot",
        "clean": "npm cache clean && npm run rimraf -- node_modules doc coverage dist ngfactory aot",
        "e2e": "npm run protractor",
        "ngc": "ngc -p tsconfig.aot.json",
        "prebuild:prod": "npm run clean:dist",
        "prebuild:aot": "npm run clean:dist",
        "prengc": "npm run clean:ngc",
        "pree2e": "npm run webdriver:update -- --standalone",
        "protractor": "protractor",
        "rimraf": "rimraf",
        "rollup": "rollup -c rollup.config.js",
        "start": "webpack-dev-server --progress --watch",
        "server": "http-server dist --cors",
        "test": "karma start",
        "tslint": "tslint",
        "webdriver-manager": "webdriver-manager",
        "webdriver:start": "npm run webdriver-manager start",
        "webdriver:update": "npm run webdriver-manager update"
    },
    "license": "ISC",
    "dependencies": {
        "@angular/common": "^2.1.2",
